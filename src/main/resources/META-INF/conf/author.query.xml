<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:exc="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-3.0">
    <execution>
        <exc:sql>
            <exc:items-query>SELECT :select FROM author</exc:items-query>
            <exc:count-query>SELECT count(*) FROM author</exc:count-query>
        </exc:sql>
    </execution>
    <fields>
        <field>
            <id>id</id>
            <domain>integer</domain>
            <display/>
        </field>
        <field>
            <id>name</id>
            <domain>string</domain>
            <display/>
        </field>
    </fields>
</query>
